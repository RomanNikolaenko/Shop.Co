{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2018",
      "dom"
    ],
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./src",
    "outDir": "./dist/shop-co/server",
    "paths": {
      "^assets": [
        "./assets"
      ],
      "^assets/*": [
        "./assets/*"
      ],
      "^core": [
        "./app/core"
      ],
      "^core/*": [
        "./app/core/*"
      ],
      "^directives": [
        "./app/directives"
      ],
      "^directives/*": [
        "./app/directives/*"
      ],
      "^environments": [
        "./app/environments"
      ],
      "^environments/*": [
        "./app/environments/*"
      ],
      "^features": [
        "./app/features"
      ],
      "^features/*": [
        "./app/features/*"
      ],
      "^guards": [
        "./app/guards"
      ],
      "^guards/*": [
        "./app/guards/*"
      ],
      "^interceptors": [
        "./app/interceptors"
      ],
      "^interceptors/*": [
        "./app/interceptors/*"
      ],
      "^interfaces": [
        "./app/interfaces"
      ],
      "^interfaces/*": [
        "./app/interfaces/*"
      ],
      "^layouts": [
        "./app/layouts"
      ],
      "^layouts/*": [
        "./app/layouts/*"
      ],
      "^models": [
        "./app/models"
      ],
      "^models/*": [
        "./app/models/*"
      ],
      "^services": [
        "./app/services"
      ],
      "^services/*": [
        "./app/services/*"
      ],
      "^shared": [
        "./app/shared"
      ],
      "^shared/*": [
        "./app/shared/*"
      ]
    },
  },
  "files": [
    "./src/main.server.ts"
  ],
  "angularCompilerOptions": {
    "enableIvy": true
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "**/*.spec.ts",
    "**/*.test.ts",
    "__tests__",
    "src/main.ts",
    "src/server.ts"
  ],
  "references": [
    {
      "path": "./tsconfig.server.app.json"
    },
    {
      "path": "./tsconfig.server.spec.json"
    }
  ]
}